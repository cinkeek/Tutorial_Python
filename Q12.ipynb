   "source": [
    "s = input(\"Enter string: \")\n",
    "\n",
    "# Palindrome\n",
    "if s == s[::-1]:\n",
    "    print(\"Palindrome\")\n",
    "else:\n",
    "    print(\"Not Palindrome\")\n",
    "\n",
    "# Symmetrical\n",
    "mid = len(s)//2\n",
    "if s[:mid] == s[mid:]:\n",
    "    print(\"Symmetrical\")\n",
    "else:\n",
    "    print(\"Not Symmetrical\")\n"
   ]
